{
  "name": "listings_ebay",
  "version": "1.0.0",
  "description": "automation for listing ebay items",
  "main": "electron/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w sqlite3 ",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/magnifyingglass.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/magnifyingglass.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Listing Automation\""
  },
  "keywords": [
    "ebay",
    "listing",
    "automation"
  ],
  "author": "Ivan",
  "license": "ISC",
  "dependencies": {
    "chrome-cookies-secure": "^1.3.3",
    "puppeteer": "^13.1.2",
    "tesseract.js": "^2.1.5"
  },
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-packager": "^15.4.0",
    "electron-rebuild": "^3.2.7"
  }
}
